{
    "input_dim": 10,
    "layers": [
        {
            "activation": "ReLU",
            "units": 16,
            "name": "layer1"
        },
        {
            "activation": "ReLU",
            "units": 8,
            "name": "layer2"
        },
        {
            "activation": "Linear",
            "units": 2,
            "name": "layer3"
        },
        {
            "activation": "Softmax",
            "units": 2,
            "name": "output_layer"
        }
    ],
    "loss": "CategoricalCrossEntropy",
    "optimizer": "Adam",
    "lr": 0.05,
    "metrics": {"accuracy": "categorical_accuracy"}
}